{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "properties": {
    "title": {
      "type": "string"
    },
    "subtitle": {
      "type": "string"
    },
    "annotations": {
      "type": "string"
    },
    "updatedDate": {
      "type": "string"
    },
    "intermediate": {
      "type": "boolean"
    },
    "threshold": {
      "type": "string"
    },
    "parties": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "name": {
            "type": "string"
          },
          "colour": {
            "type": "string"
          },
          "percentage": {
            "type": "number"
          },
          "previous": {
            "type": "number"
          }
        }
      }
    },
    "notes": {
      "type": "string"
    },
    "sources": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "text": {
            "type": "string"
          },
          "href": {
            "type": "string"
          },
          "validHref": {
            "type": "boolean"
          }
        }
      }
    }
  },
  "required": [
    "title"
  ]
}
